{
  "name": "WoW Armory Links",
  "description": "Adds shortcuts to WoW armory.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*.worldofwarcraft.com/*/character/*"],
      "js": ["main.js"]
    }
  ],
  "host_permissions": ["https://worldofwarcraft.com/"],
  "web_accessible_resources": [
    {
      "resources": ["images/raider-io.svg", "images/warcraft-logs.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
